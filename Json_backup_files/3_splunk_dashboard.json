        "query": "index=main sourcetype=\"access.log\"\n| rex \"^(?<client_ip>[^ ]+) [^ ]+ [^ ]+ \\[(?<timestamp>[^\\]]+)\\] \\\"(?<method>[^\\\"]+)\\\" (?<status>\\d{3}) (?<bytes>\\d+)\"\n| eval status = tonumber(status)\n| where status = status\n| table client_ip, timestamp, method, status, bytes",
        "queryParameters": {
          "earliest": "-7d@h",
          "latest": "now",
          "sampleRatio": 1
        }
      },
      "name": "DETAILS - Table search"
