    "dataSources": {
        "ds_7qOSF9zP": {
            "name": "Containers with Most ERROR Logs - Column chart search",
            "options": {
                "query": "source=\"Docker-container-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"docker container logs\"\n| rex field=_raw \"^(?<timestamp>\\S+)\\s+(?<container>\\S+)\\s+(?<level>INFO|WARN|ERROR)\"\n| where level=\"ERROR\"\n| stats count by container\n| sort - count",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_MWpNAOYi": {
            "name": "Firewall Actions Count - Column chart search",
            "options": {
                "query": "source=\"firewall-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"firewall-logs\"\n| rex field=_raw \"^(?<timestamp>\\w+\\s+\\d+\\s+\\d+:\\d+:\\d+)\\s+(?<host>\\S+)\\s+(?<action>DROP|ACCEPT)\"\n| stats count by action",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_UQFlQWXJ": {
            "name": "DETAILED VIEW OF FIREWALL ACTIONS - Table search",
            "options": {
                "query": "source=\"firewall-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"firewall-logs\"\n| rex field=_raw \"^(?<timestamp>\\w+\\s+\\d+\\s+\\d+:\\d+:\\d+)\\s+(?<host>\\S+)\\s+(?<action>DROP|ACCEPT)\\s+(?<protocol>\\S+)\\s+(?<src_ip>\\d+\\.\\d+\\.\\d+\\.\\d+):(?<src_port>\\d+)\\s+->\\s+(?<dest_ip>\\d+\\.\\d+\\.\\d+\\.\\d+):(?<dest_port>\\d+)\"\n| where action=\"$selected_action$\"\n| table timestamp, host, action, protocol, src_ip, src_port, dest_ip, dest_port\n| sort - timestamp",
                "queryParameters": {
                    "earliest": "0",
                    "latest": "",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_enVgHCRl": {
            "name": "Logs by level - Table search",
            "options": {
                "query": "source=\"Docker-container-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"docker container logs\"\n| rex field=_raw \"^(?<timestamp>\\S+)\\s+(?<container>\\S+)\\s+(?<level>INFO|WARN|ERROR)\\s+(?<message>.+)\"\n| stats count by container, level",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_gB7fnZ8s": {
            "name": "Protocol-wise Action Count - Bar chart search",
            "options": {
                "query": "source=\"firewall-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"firewall-logs\"\n| rex field=_raw \"^(?<timestamp>\\w+\\s+\\d+\\s+\\d+:\\d+:\\d+)\\s+\\S+\\s+(?<action>DROP|ACCEPT)\\s+(?<protocol>\\S+)\"\n| stats count by protocol, action",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_h5anD3iC": {
            "name": "Detailed view of the Container - Table search",
            "options": {
                "query": "source=\"Docker-container-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"docker container logs\"\n| rex field=_raw \"^(?<timestamp>\\S+)\\s+(?<container>\\S+)\\s+(?<level>INFO|WARN|ERROR)\\s+(?<message>.+)\"\n| where level=\"ERROR\" AND container=\"$selected_container$\"\n| table timestamp, container, level, message\n| sort - timestamp",
                "queryParameters": {
                    "earliest": "0",
                    "latest": "",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_jArVMONN": {
            "name": "Most Targeted Destination Ports - Column chart search",
            "options": {
                "query": "source=\"firewall-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"firewall-logs\"\n| rex field=_raw \"->\\s+(?:\\d+\\.\\d+\\.\\d+\\.\\d+):(?<dest_port>\\d+)\"\n| stats count by dest_port\n| sort - count\n| head 10",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_pK5TUeZM": {
            "name": "Detailed Firewall Logs - Table search",
            "options": {
                "query": "source=\"firewall-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"firewall-logs\"\n| rex field=_raw \"^(?<timestamp>\\w+\\s+\\d+\\s+\\d+:\\d+:\\d+)\\s+(?<host>\\S+)\\s+(?<action>DROP|ACCEPT)\\s+(?<protocol>\\S+)\\s+(?<src_ip>\\d+\\.\\d+\\.\\d+\\.\\d+):(?<src_port>\\d+)\\s+->\\s+(?<dest_ip>\\d+\\.\\d+\\.\\d+\\.\\d+):(?<dest_port>\\d+)\"\n| table timestamp, host, action, protocol, src_ip, src_port, dest_ip, dest_port\n| sort - timestamp",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_s6SVKAyP": {
            "name": "Logs Over Time - Line chart search",
            "options": {
                "query": "source=\"Docker-container-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"docker container logs\"\n| rex field=_raw \"^(?<timestamp>\\S+)\\s+(?<container>\\S+)\\s+(?<level>INFO|WARN|ERROR)\"\n| timechart count by level span=1h",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        }
