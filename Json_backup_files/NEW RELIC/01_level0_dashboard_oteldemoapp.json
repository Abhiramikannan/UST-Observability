            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT\r\n  average(duration.ms) AS 'Avg Resp Time (ms)',\r\n  rate(count(*), 1 second) AS 'Req/s',\r\n  percentage(count(*), WHERE error = true) AS 'Error (%)'\r\nFACET service.name\r\nORDER BY 'Error (%)' DESC\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
