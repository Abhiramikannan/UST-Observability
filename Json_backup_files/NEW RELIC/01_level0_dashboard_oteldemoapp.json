      "name": "Service SLO",
      "description": null,
      "widgets": [
        {
          "id": "498266265",
          "title": "SLO compliance (%)",
          "layout": {
            "column": 1,
            "row": 1,
            "width": 6,
            "height": 3
          },
          "linkedEntityGuids": [],
          "visualization": {
            "id": "viz.table"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "nrqlQueries": [
              {
                "accountIds": [
                  7291193
                ],
                "query": "FROM Span\r\nSELECT\r\n  (100 - percentage(count(*), WHERE duration.ms > 1000)) AS 'Latency SLO (%)',\r\n  (100 - percentage(count(*), WHERE http.status_code >= 400)) AS 'Error SLO (%)'\r\nFACET service.name\r\nORDER BY 'Latency SLO (%)' ASC\r\nLIMIT 100\r\n"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            }
          }
        },
        {
          "id": "498267677",
          "title": "Services Overview",
          "layout": {
