{
  "title": "Abhi Enterprise Security Incident Dashboard",
  "description": "",
  "inputs": {
    "input_global_trp": {
      "options": {
        "defaultValue": "-24h@h,now",
        "token": "global_time"
      },
      "title": "Global Time Range",
      "type": "input.timerange"
    }
  },
  "defaults": {
    "dataSources": {
      "ds.search": {
        "options": {
          "queryParameters": {
            "earliest": "$global_time.earliest$",
            "latest": "$global_time.latest$"
          }
        }
      }
    },
    "visualizations": {
      "global": {
        "showProgressBar": true
      }
    }
  },
  "visualizations": {
    "viz_aHRsrJey": {
      "dataSources": {
        "primary": "ds_AnU0vgfc"
      },
      "options": {
