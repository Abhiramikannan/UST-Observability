        "y2AxisTitleVisibility": "show",
        "yAxisAbbreviation": "auto",
        "yAxisScale": "linear",
        "yAxisTitleVisibility": "show"
      },
      "title": "Alerts by Severity",
      "type": "splunk.column"
    },
    "viz_t3rpWF0M": {
      "containerOptions": {},
      "dataSources": {
        "primary": "ds_YAiqTzDw"
      },
      "eventHandlers": [
        {
          "options": {
            "newTab": true,
            "type": "auto"
          },
          "type": "drilldown.linkToSearch"
        },
        {
          "options": {
            "tokens": [
              {
                "key": "row.<fieldname>.value",
                "token": "severity_token"
              }
            ]
          },
          "type": "drilldown.setToken"
        }
      ],
      "showLastUpdated": false,
      "showProgressBar": true,
      "title": "Incident Severity vs Status Overview",
      "type": "splunk.bar"
    }
  },
  "dataSources": {
    "ds_1YtQ7gat": {
      "name": "Alerts by Severity search",
      "options": {
        "query": "source=\"security dashboard.csv\" index=\"main\"\r\n| stats count by alert_severity",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "type": "ds.search"
    },
    "ds_AnU0vgfc": {
      "name": "Vulnerability Status Breakdown search",
      "options": {
        "query": "source=\"security dashboard.csv\" index=\"main\"\r\n| stats count by vuln_type, vuln_status",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "type": "ds.search"
    },
    "ds_UpjWKnBT": {
      "name": "Compliance Status Summary search",
      "options": {
        "query": "source=\"security dashboard.csv\" index=\"main\"\r\n| stats count by compliance_area, compliance_status",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "type": "ds.search"
    },
    "ds_YAiqTzDw": {
      "name": "Incident Overview - Table search",
      "options": {
        "query": "source=\"security dashboard.csv\" index=\"main\" \r\n| stats count by severity, status",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "type": "ds.search"
    },
    "ds_gFpbJJKp": {
      "name": "Login Success Rate search",
      "options": {
        "query": "source=\"security dashboard.csv\" index=\"main\"\r\n| stats count by login_success",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "type": "ds.search"
    },
    "ds_rZgWCySz": {
      "name": "Open Critical Incidents search",
      "options": {
        "query": "source=\"security dashboard.csv\" index=\"main\" (severity=\"Critical\" OR severity=\"High\") status=\"Open\"\r\n| table timestamp, incident_id, incident_type, severity, status, user_id",
        "queryParameters": {
          "earliest": "0",
          "sampleRatio": 1
        }
      },
      "type": "ds.search"
    }
  },
