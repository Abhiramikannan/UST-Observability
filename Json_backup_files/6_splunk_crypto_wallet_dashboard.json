    "dataSources": {
        "ds_1TyRBzQm": {
            "name": "Outgoing Transfers per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| where like(_raw, \"Transferred%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as outgoing_transfers by currency",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_1sT85znm": {
            "name": "Number of Transfers per Wallet - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| where like(_raw, \"Transferred%\")\n| eval wallet_from=mvindex(split(_raw,\" \"),6)\n| eval wallet_to=mvindex(split(_raw,\" \"),13)\n| stats count as transfers by wallet_from\n| sort -transfers",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_4DdebnyF": {
            "name": "Error _ - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  earliest=-24h latest=now \n| eval is_error=if(like(_raw,\"%not found%\"),1,0)\n| stats count as total_events sum(is_error) as total_errors\n| eval error_percentage = round((total_errors/total_events)*100,2)\n| fields error_percentage",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_59AbgRT3": {
            "name": "Total Number of Transfers - Single value search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| where like(_raw, \"Transferred%\")\n| stats count as total_transfers",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_6JXE8DH7": {
            "name": "Detailed view - Table search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\" earliest=-24h latest=now\n            | eval category=case(\n                match(_raw,\"Created wallet\"),\"wallets_created\",\n                match(_raw,\"Deleted wallet\"),\"wallets_deleted\",\n                match(_raw,\"Deposited\"),\"deposits\",\n                match(_raw,\"Withdrew\"),\"withdrawals\",\n                match(_raw,\"Transferred\"),\"transfers\",\n                like(_raw,\"%not found%\"),\"errors\",\n                true(),\"events\")\n            | search category=$details$\n            | table _time _raw",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_DGBEsTcG": {
            "name": "Total Number of Withdrawals - Single value search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| where like(_raw, \"Withdrew%\")\n| stats count as total_withdrawals",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_F2Ts5qo3": {
            "name": "Number of Withdrawals per Wallet - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| where like(_raw, \"Withdrew%\")\n| eval wallet_id=mvindex(split(_raw,\" \"),6)\n| stats count as withdrawals by wallet_id\n| sort -withdrawals",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_IFu3C4bN": {
            "name": "Total number of Transactions - Single value search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| eval is_transaction=if(like(_raw,\"Deposited%\") OR like(_raw,\"Withdrew%\") OR like(_raw,\"Transferred%\"), 1, 0)\n| stats sum(is_transaction) as total_transactions",
                "queryParameters": {
                    "earliest": "-24h@h",
                    "latest": "now",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_O50oNIMl": {
            "name": "Total Number of Deposits - Single value search",
            "options": {
                "query": "index=\"otel\" wallet earliest=-24h latest=now\n| where like(_raw, \"Deposited%\")\n| stats count as total_deposits",
                "queryParameters": {
