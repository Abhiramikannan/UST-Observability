    "dataSources": {
        "ds_6Fn0Mlec": {
            "name": "Total events - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"  \n| stats count AS \"Total Events\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_7a2Q7eh6": {
            "name": "Outgoing Transfers per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet \n| where like(_raw, \"Transferred%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as outgoing_transfers by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_9keTHka3": {
            "name": "Failed Operations - Single value search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"   \"not found\"\n| stats count AS \"Failed Operations\"",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_9oPDY7uo": {
            "name": "Withdrawals per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet\n| where like(_raw, \"Withdrew%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as withdrawals by currency",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_A4X3gv8X": {
            "name": "Detailed view - Table search",
            "options": {
                "query": "index=otel host=\"cryptowallet-84d755cfdb-qvb5x\"\n            | eval category=case(\n                match(_raw,\"Created wallet\"),\"wallets_created\",\n                match(_raw,\"Deleted wallet\"),\"wallets_deleted\",\n                match(_raw,\"Deposited\"),\"deposits\",\n                match(_raw,\"Withdrew\"),\"withdrawals\",\n                match(_raw,\"Transferred\"),\"transfers\",\n                like(_raw,\"%not found%\"),\"errors\",\n                true(),\"events\")\n            | search category=$details$\n            | table _time _raw",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_BEMLGtJO": {
            "name": "Number of Transactions per Currency - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet \n| eval type=case(\n    match(_raw,\"Deposited\"),\"Deposit\",\n    match(_raw,\"Withdrew\"),\"Withdrawal\",\n    match(_raw,\"Transferred\"),\"Transfer\"\n)\n| rex field=_raw \"Deposited\\s+\\d+\\s+(?<deposit_currency>\\w+)\"\n| rex field=_raw \"Withdrew\\s+\\d+\\s+(?<withdraw_currency>\\w+)\"\n| rex field=_raw \"Transferred\\s+\\d+\\s+(?<transfer_currency_from>\\w+).*to\\s+\\d+\\s+(?<transfer_currency_to>\\w+)\"\n| eval currency=coalesce(deposit_currency, withdraw_currency, transfer_currency_from)\n| stats count as transactions by currency\n| sort -transactions",
                "queryParameters": {
                    "earliest": "-1d@d",
                    "latest": "@d",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_BFBOB2A8": {
