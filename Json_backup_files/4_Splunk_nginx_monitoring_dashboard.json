    "dataSources": {
        "ds_4nrSsMCI": {
            "name": "Total Requests Per Hour - Line chart search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| timechart count span=1h",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_8AnLVpsO": {
            "name": "Top 10 Client IPs - Column chart search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| rex field=_raw \"^(?<client_ip>\\d+\\.\\d+\\.\\d+\\.\\d+)\"\n| stats count by client_ip\n| sort -count\n| head 10",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_AglDpyjV": {
            "name": "Requests by HTTP Method - Pie chart search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| rex field=_raw \"\\\"(?<method>\\w+)\\s\"\n| stats count by method",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_E0sIz83y": {
            "name": "Client _ Server Errors Over Time - Area chart search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| rex field=_raw \"\\\"\\s(?<status_code>\\d{3})\\s\"\n| search status_code>=400\n| timechart count by status_code span=1h",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_Gpghxb5x": {
            "name": "Top 10 Requested Endpoints - Column chart search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| rex field=_raw \"\\\"\\w+\\s(?<uri>/\\S*)\\sHTTP\"\n| stats count by uri\n| sort -count\n| head 10",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_boQUCmuZ": {
            "name": "DETAILED DATA OF REQ - Table search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| rex field=_raw \"^(?<client_ip>\\d+\\.\\d+\\.\\d+\\.\\d+)\"\n| rex field=_raw \"\\[(?<timestamp>[^\\]]+)\\]\"\n| rex field=_raw \"\\\"(?<method>GET|POST)\\s(?<uri>/\\S*)\\sHTTP\"\n| rex field=_raw \"\\\"\\s(?<status_code>\\d{3})\\s(?<bytes>\\d+)\"\n| where method=\"$selected_method$\"\n| table timestamp, client_ip, method, uri, status_code, bytes\n| sort - timestamp",
                "queryParameters": {
                    "earliest": "0",
                    "latest": "",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_e2n9qjJ6": {
            "name": "Requests by HTTP Status Code - Column chart search",
            "options": {
                "query": "source=\"nginx-access-logs.log\" host=\"si-i-0d6634ad86707eee5.prd-p-i0wcm.splunkcloud.com\" index=\"main\" sourcetype=\"nginx-logs\"\n| rex field=_raw \"\\\"\\s(?<status_code>\\d{3})\\s\"\n| stats count by status_code\n| sort -count",
                "queryParameters": {
                    "earliest": "0",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        }
    },
