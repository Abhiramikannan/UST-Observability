    "dataSources": {
        "ds_0fCJ9Ccx": {
            "name": "Outgoing Transfers per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet \n| where like(_raw, \"Transferred%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as outgoing_transfers by currency",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_5nxg4RR1": {
            "name": "Total events - Single value search",
            "options": {
                "query": "index=otel\n| eval is_transaction=if(match(_raw,\"Deposited\") OR match(_raw,\"Withdrew\") OR match(_raw,\"Transferred\"), 1, 0)\n| where is_transaction=1\n| stats count AS \"Total Transactions\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_9dRTvazw": {
            "name": "Total Number of Withdrawals - Single value search",
            "options": {
                "query": "index=\"otel\" wallet \n| where like(_raw, \"Withdrew%\")\n| stats count as total_withdrawals",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_CXVt0niR": {
            "name": "Withdrawals per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet\n| where like(_raw, \"Withdrew%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as withdrawals by currency",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_EnIroU29": {
            "name": "Deposits per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet \n| where like(_raw, \"Deposited%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as deposits by currency",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_EpKy0VQH": {
            "name": "Total Errors_wallet not found_ - Single value search",
            "options": {
                "query": "index=otel host=cryptowallet-*  \"not found\"\n| stats count as \"Total Errors\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_IxMAnN5S": {
            "name": "Error _ - Single value search",
            "options": {
                "query": "index=otel host=cryptowallet-* | eval is_error=if(like(_raw,\"%not found%\"),1,0) | stats count as total_events sum(is_error) as total_errors | eval error_percentage = round((total_errors/total_events)*100,2) | fields error_percentage",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_V7UmaMNI": {
            "name": "Total wallets deleted - Single value search",
            "options": {
                "query": "index=otel  \"Deleted wallet\" | stats count as \"Total Wallets Deleted\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_dCpEAGrQ": {
            "name": "Failed Operations - Single value search",
            "options": {
                "query": "index=otel host=cryptowallet-* \"not found\"\n| stats count AS \"Failed Operations\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_io13JnNi": {
            "name": "Total Number of Deposits - Single value search",
            "options": {
                "query": "index=\"otel\" wallet\n| where like(_raw, \"Deposited%\")\n| stats count as total_deposits",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_klrNPvm8": {
            "name": "Total number of Transactions - Single value search",
            "options": {
                "query": "index=\"otel\" wallet \n| eval is_transaction=if(like(_raw,\"Deposited%\") OR like(_raw,\"Withdrew%\") OR like(_raw,\"Transferred%\"), 1, 0)\n| stats sum(is_transaction) as total_transactions",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_qLaTqKN7": {
            "name": "Total wallet created - Single value search",
            "options": {
                "query": "index=otel  \"Created wallet\"\n\n| stats count as \"Total Wallets Created\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_qZlFmZpA": {
            "name": "Transactions by Location and Type - Choropleth map search",
            "options": {
                "query": "index=otel \"wallet\"\n| rex field=_raw \"Transferred\\s+(?<amount>\\d+)\\s+(?<currency>[A-Z]{3})\\s+from wallet\\s+(?<from_wallet>[0-9a-f-]+)\\s+at location\\s+(?<from_location>[^,]+),\\s+latitude\\s+(?<from_lat>[-\\d\\.]+),\\s+longitude\\s+(?<from_lon>[-\\d\\.]+)\\s+to\\s+(?<to_amount>\\d+)\\s+(?<to_currency>[A-Z]{3})\\s+in wallet\\s+(?<to_wallet>[0-9a-f-]+)\\s+at location\\s+(?<to_location>[^,]+),\\s+latitude\\s+(?<to_lat>[-\\d\\.]+),\\s+longitude\\s+(?<to_lon>[-\\d\\.]+)\"\n| eval amount=tonumber(amount), to_amount=tonumber(to_amount)\n| eval from_lat=tonumber(from_lat), from_lon=tonumber(from_lon), to_lat=tonumber(to_lat), to_lon=tonumber(to_lon)\n| geostats latfield=from_lat longfield=from_lon count by currency",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_rernWqsW": {
            "name": "Total Number of Transfers - Single value search",
            "options": {
                "query": "index=\"otel\" wallet \n| where like(_raw, \"Transferred%\")\n| stats count as total_transfers",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_vk4elBnt": {
            "name": "Number of Transactions per Currency - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet \n| eval type=case(\n    match(_raw,\"Deposited\"),\"Deposit\",\n    match(_raw,\"Withdrew\"),\"Withdrawal\",\n    match(_raw,\"Transferred\"),\"Transfer\"\n)\n| rex field=_raw \"Deposited\\s+\\d+\\s+(?<deposit_currency>\\w+)\"\n| rex field=_raw \"Withdrew\\s+\\d+\\s+(?<withdraw_currency>\\w+)\"\n| rex field=_raw \"Transferred\\s+\\d+\\s+(?<transfer_currency_from>\\w+).*to\\s+\\d+\\s+(?<transfer_currency_to>\\w+)\"\n| eval currency=coalesce(deposit_currency, withdraw_currency, transfer_currency_from)\n| stats count as transactions by currency\n| sort -transactions",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        }
