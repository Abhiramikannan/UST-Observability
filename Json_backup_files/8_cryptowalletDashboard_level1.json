    "dataSources": {
        "ds_6uxjN4Ss": {
            "name": "Error _ - Single value search",
            "options": {
                "query": "index=otel host=cryptowallet-* | eval is_error=if(like(_raw,\"%not found%\"),1,0) | stats count as total_events sum(is_error) as total_errors | eval error_percentage = round((total_errors/total_events)*100,2) | fields error_percentage",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_BBby2n9t": {
            "name": "Total Errors_wallet not found_ - Single value search",
            "options": {
                "query": "index=otel host=cryptowallet-*  \"not found\"\n| stats count as \"Total Errors\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_DgPV0zGp": {
            "name": "Deposits per Currency_ - Column chart search",
            "options": {
                "query": "index=\"otel\" wallet \n| where like(_raw, \"Deposited%\")\n| eval currency=mvindex(split(_raw,\" \"),2)\n| stats count as deposits by currency",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_IparLHO0": {
            "name": "Failed Operations - Single value search",
            "options": {
                "query": "index=otel host=cryptowallet-* \"not found\"\n| stats count AS \"Failed Operations\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_J08EnZGw": {
            "name": "Transactions by Location and Type - Choropleth map search",
            "options": {
                "query": "index=otel \"wallet\"\n| rex field=_raw \"Transferred\\s+(?<amount>\\d+)\\s+(?<currency>[A-Z]{3})\\s+from wallet\\s+(?<from_wallet>[0-9a-f-]+)\\s+at location\\s+(?<from_location>[^,]+),\\s+latitude\\s+(?<from_lat>[-\\d\\.]+),\\s+longitude\\s+(?<from_lon>[-\\d\\.]+)\\s+to\\s+(?<to_amount>\\d+)\\s+(?<to_currency>[A-Z]{3})\\s+in wallet\\s+(?<to_wallet>[0-9a-f-]+)\\s+at location\\s+(?<to_location>[^,]+),\\s+latitude\\s+(?<to_lat>[-\\d\\.]+),\\s+longitude\\s+(?<to_lon>[-\\d\\.]+)\"\n| eval amount=tonumber(amount), to_amount=tonumber(to_amount)\n| eval from_lat=tonumber(from_lat), from_lon=tonumber(from_lon), to_lat=tonumber(to_lat), to_lon=tonumber(to_lon)\n| geostats latfield=from_lat longfield=from_lon count by currency",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_N7g9KfDL": {
            "name": "Total wallet created - Single value search",
            "options": {
                "query": "index=otel  \"Created wallet\"\n\n| stats count as \"Total Wallets Created\"",
                "queryParameters": {
                    "earliest": "$time.earliest$",
                    "latest": "$time.latest$",
                    "sampleRatio": 1
                }
            },
            "type": "ds.search"
        },
        "ds_V3gtH18u": {
            "name": "Withdrawals per Currency_ - Column chart search",
            "options": {
